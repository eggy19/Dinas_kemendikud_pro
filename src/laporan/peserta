     tb_peserta.`jenkel` AS tb_peserta_jenkel,
     tb_peserta.`asal_sekolah` AS tb_peserta_asal_sekolah,
     tb_peserta.`bidang_lomba` AS tb_peserta_bidang_lomba,
     tb_peserta.`telp_sekolah` AS tb_peserta_telp_sekolah,
     tb_peserta.`alamat_sekolah` AS tb_peserta_alamat_sekolah,
     tb_peserta.`provinsi` AS tb_peserta_provinsi,
     tb_peserta.`alamat_rumah` AS tb_peserta_alamat_rumah,
     tb_peserta.`hp_pribadi` AS tb_peserta_hp_pribadi
FROM
     `tb_peserta` tb_peserta]]>
